{
  "manifest_version": 3,

  "name": "Whisperer",
  "version": "1.0",

  "description": "Lateral blog post demo extension",

  "content_scripts" : [
    {
      "js": ["contentscript.js"],
      "matches": ["<all_urls>"]
    }
  ],
  
  "web_accessible_resources": [{
    "resources": [ "mathScript.js" ],
    "matches": [ "<all_urls>" ]
  }],

  "action": {
    "default_icon": "assets/test-icon.png",
    "default_title": "Whisperer popup to be renamed",
    "default_popup": "popup.html"
  }

}