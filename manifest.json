{
  "manifest_version": 3,

  "name": "Whisperer",
  "version": "1.0",

  "description": "Lateral blog post demo extension",

  "content_scripts" : [
    {
      "js": ["src/contentscript.js"],
      "matches": ["<all_urls>"]
    }
  ],

  "action": {
    "default_icon": "img/search.png",
    "default_title": "Whisperer popup to be renamed",
    "default_popup": "src/whisper.html"
  }

}